# Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration (if using ChatGPT)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Custom ChatBot API Configuration
VITE_CHATBOT_API_URL=http://localhost:3001/api
VITE_CHATBOT_API_KEY=your_custom_api_key_here

# Application Configuration
VITE_APP_TITLE=My Chat Bot
VITE_APP_DESCRIPTION=A modern chat interface

# SSO Configuration
# Origin URL for SSO messages (update this to match your SSO provider)
VITE_APP_ORIGIN=https://your-sso-provider.com

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000

# Development Configuration
VITE_DEV_MODE=true
